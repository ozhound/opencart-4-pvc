          <div class="row mb-3 required">
            <label class="col-sm-2 col-form-label" for="input-vendor" data-bs-toggle="tooltip" title="{{ entry_companyid_help }}">{{ entry_companyid }}</label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_companyid" value="{{ payment_begateway_companyid }}" placeholder="{{ entry_companyid }}" id="input-vendor" class="form-control" />
            </div>
          </div>
          <div class="row mb-3 required">
            <label class="col-sm-2 col-form-label" for="input-key" data-bs-toggle="tooltip" title="{{ entry_encyptionkey_help }}">{{ entry_encyptionkey }}</label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_encyptionkey" value="{{ payment_begateway_encyptionkey }}" placeholder="{{ entry_encyptionkey }}" id="input-key" class="form-control" />
            </div>
          </div>
          <div class="row mb-3 required">
            <label class="col-sm-2 col-form-label" for="input-publickey" data-bs-toggle="tooltip" title="{{ entry_publickey_help }}">{{ entry_publickey }}</label>
            <div class="col-sm-10">
              <textarea name="payment_begateway_publickey" rows="4" placeholder="{{ entry_publickey }}" id="input-publickey" class="form-control" />{{ payment_begateway_publickey }}</textarea>
            </div>
          </div>
          <div class="row mb-3 required">
            <label class="col-sm-2 col-form-label" for="input-domain-page" data-bs-toggle="tooltip" title="{{ entry_domain_payment_page_help }}">{{ entry_domain_payment_page }}</label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_domain_payment_page" value="{{ payment_begateway_domain_payment_page }}" placeholder="{{ entry_domain_payment_page }}" id="input-domain-page" class="form-control" />
            </div>
          </div>
          <div class="row mb-3 required">
            <label class="col-sm-2 col-form-label" for="input-expiry" data-bs-toggle="tooltip" title="{{ entry_expiry_help }}">{{ entry_expiry }}</label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_expiry" value="{{ payment_begateway_expiry }}" placeholder="{{ entry_expiry }}" id="input-expiry" class="form-control" />
            </div>
          </div>

{% for l in languages %}
          <div class="row mb-3">
            <label class="col-sm-2 col-form-label" for="input-heading-title-{{ l.code }}" data-bs-toggle="tooltip" title="{{ l.entry_heading_title_help }}">{{ l.entry_heading_title }} <img src="{{l.image}}"></label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_heading_title_{{ l.code }}" value="{{ l.heading_title }}" placeholder="{{ l.heading_title }}" id="input-heading-title-{{ l.code }}" class="form-control" />
            </div>
          </div>
          <div class="row mb-3">
            <label class="col-sm-2 col-form-label" for="input-method-title-{{ l.code }}" data-bs-toggle="tooltip" title="{{ l.entry_method_title_help }}">{{ l.entry_method_title }} <img src="{{l.image}}"></label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_method_title_{{ l.code }}" value="{{ l.method_title }}" placeholder="{{ l.method_title }}" id="input-method-title-{{ l.code }}" class="form-control" />
            </div>
          </div>
{% endfor %}

          <div class="row mb-3">
            <label class="col-sm-2 col-form-label" for="begateway_payment_type_card">{{ entry_payment_type_card }}</label>
            <div class="col-sm-10">
              <div class="form-check form-switch form-switch-lg">
                <input type="hidden" name="payment_begateway_payment_type[card]" value="0" />
                <input type='checkbox' name='payment_begateway_payment_type[card]' class="form-check-input" id="begateway_payment_type_card" value='1' {{ payment_begateway_payment_type.card is defined and payment_begateway_payment_type.card == 1 ? 'checked="checked"' : '' }} />
              </div>
            </div>
          </div>
          <div class="row mb-3">
            <label class="col-sm-2 col-form-label" for="begateway_payment_type_halva">{{ entry_payment_type_halva }}</label> 
            <div class="col-sm-10">        

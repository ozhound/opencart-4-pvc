                <input type='checkbox' name='payment_begateway_payment_type[erip]' class="form-check-input" id="begateway_payment_type_erip" value='1' {{ payment_begateway_payment_type.erip is defined and payment_begateway_payment_type.erip == 1 ? 'checked="checked"' : '' }} />
              </div>
            </div>
          </div>
          <div class="row mb-3">
            <label class="col-sm-2 col-form-label" for="erip_service_no-key">{{ entry_payment_begateway_erip_service_no }}</label>
            <div class="col-sm-10">
              <input type="text" name="payment_begateway_erip_service_no" value="{{ payment_begateway_erip_service_no }}" placeholder="{{ entry_payment_begateway_erip_service_no }}" id="erip_service_no" class="form-control" />
            </div>
          </div>
          <div class="row mb-3">

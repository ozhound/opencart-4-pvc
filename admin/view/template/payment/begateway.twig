                <input type="hidden" name="payment_begateway_payment_type[halva]" value="0" />
                <input type='checkbox' name='payment_begateway_payment_type[halva]' class="form-check-input" id="begateway_payment_type_halva" value='1' {{ payment_begateway_payment_type.halva is defined and payment_begateway_payment_type.halva == 1 ? 'checked="checked"' : '' }} />
              </div>
            </div>
          </div>   
